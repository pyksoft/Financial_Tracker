<Window x:Class="Financial_Tracker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <TextBox x:Name="salarybox" Width="200" Height="20" Margin="0,50,0,10" >Salary</TextBox>
        <TextBox x:Name="loansbox" Width="200" Height="20" Margin="0,0,0,10">Loans</TextBox>
        <TextBox x:Name="debtsbox" Width="200" Height="20" Margin="0,0,0,10">Debts</TextBox>
        <TextBox x:Name="utilitybox" Width="200" Height="20" Margin="0,0,0,10">Utilities</TextBox>
        <Button x:Name="submit" Width="200" Content="Submit" Click="Submit_Click">
        </Button>

    </StackPanel>
        
    
</Window>
